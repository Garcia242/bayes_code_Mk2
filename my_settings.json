{
	"[python]": {
    	"editor.defaultFormatter": "ms-python.autopep8",
	},
	"code-runner.terminalRoot":"/",
	"http.proxySupport": "off",
	"editor.inlineSuggest.enabled": true,
	"editor.formatOnPaste": true,
	"editor.formatOnSave": true,
	"prettier.useTabs": true,
	"prettier.tabWidth": 4,
	"prettier.trailingComma": "all",
	"eslint.lintTask.enable": true,
	"eslint.format.enable": true,
	"pythonIndent.trimLinesWithOnlyWhitespace": true,
	"autopep8.importStrategy": "fromEnvironment",
	"liveshare.accessibility.soundsEnabled": false,
	"editor.linkedEditing": true,
	"terminal.integrated.defaultProfile.windows": "Git Bash",
	"prettier.jsxSingleQuote": true,
	"code-runner.runInTerminal": true,
	"code-runner.saveAllFilesBeforeRun": true,
	"code-runner.saveFileBeforeRun": true,
	"workbench.iconTheme": "material-icon-theme",
	"python.testing.pytestEnabled": true,
	"cmake.showOptionsMovedNotification": false,
	"python.testing.unittestEnabled": true,
	"autoDocstring.startOnNewLine": true,
	"python.analysis.autoFormatStrings": true,
	"python.analysis.autoImportCompletions": true,
	"python.analysis.completeFunctionParens": true,
	"editor.rulers": [
		80
	],
	"autoDocstring.docstringFormat": "numpy",
	"editor.defaultFormatter": "ue.alphabetical-sorter",
	"editor.defaultFoldingRangeProvider": "ms-python.autopep8",
	"arduino.useArduinoCli": true
}